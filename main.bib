@article{Baars2018,
abstract = {In Part 1, the scaling of the streamwise velocity energy spectra in turbulent boundary layers was considered. A spectral decomposition analysis provided a means to separate out attached and non-attached eddy contributions and was used to generate three spectral sub-components, one of which is a close representation of the spectral signature induced by self-similar, wall-attached turbulence. Since sub-components of the streamwise turbulence intensity $\overline{u^2}$ follow directly from the integrated components of the velocity energy spectra, we here focus on the scaling of the former. Specific attention is given to the potential $k_x^{-1}$ behaviour in spectra, at ultra-high $Re_\tau$, and its relation with the turbulence intensity adhering to a wall-normal logarithmic decay per Townsend's attached-eddy hypothesis. This decay with a Townsend--Perry constant of $A_1 = 0.98$ is suggested to be universal across all Reynolds numbers considered. It is also demonstrated how the logarithmic-region results are consistent with the Reynolds-number increase of the streamwise turbulence intensity in the near-wall region.},
archivePrefix = {arXiv},
arxivId = {1810.03104},
author = {Baars, Woutijn J. and Marusic, Ivan},
eprint = {1810.03104},
file = {:C\:/Nutstore/Turbulence/paper/Data-driven decomposition of the streamwise turbulence kinetic energy in boundary layers.. Part 1 Energy spectra.pdf:pdf},
keywords = {1,a long-standing challenge in,amongst other statistical quantities,for the fluctuations,introduction and context,scalings of the energy,spectra,spectral coherence,the,the study of wall,turbulence has been identifying,turbulence kinetic energy,wall-bounded turbulence},
title = {{Data-driven decomposition of the streamwise turbulence kinetic energy in boundary layers. Part 2: Integrated energy and $A_1$}},
url = {http://arxiv.org/abs/1810.03104},
year = {2018}
}
