@article{Monty2007,
abstract = {In recent years there has been significant progress made towards understanding the large-scale structure of wall-bounded shear flows. Most of this work has been conducted with turbulent boundary layers, leaving scope for further work in pipes and channels. In this article the structure of fully developed turbulent pipe and channel flow has been studied using custom-made arrays of hot-wire probes. Results reveal long meandering structures of length up to 25 pipe radii or channel half-heights. These appear to be qualitatively similar to those reported in the log region of a turbulent boundary layer. However, for the channel case, large-scale coherence persists further from the wall than in boundary layers. This is expected since these large-scale features are a property of the logarithmic region of the mean velocity profile in boundary layers and it is well-known that the mean velocity in a channel remains very close to the log law much further from the wall. Further comparison of the three turbulent flows shows that the characteristic structure width in the logarithmic region of a boundary layer is at least 1.6 times smaller than that in a pipe or channel.},
author = {Monty, J. P. and Stewart, J. A. and Williams, R. C. and Chong, M. S.},
doi = {10.1017/S002211200700777X},
file = {:C\:/Nutstore/Turbulence/paper/Monty 2007 large-scale features in turbulent pipe and channel flows.pdf:pdf},
issn = {00221120},
journal = {J. Fluid Mech.},
pages = {147--156},
title = {{Large-scale features in turbulent pipe and channel flows}},
volume = {589},
year = {2007}
}
@article{Monty2009,
abstract = {ABSTRACT The extent or existence of similarities between fully developed\nturbulent pipes and channels, and in zero-pressure-gradient turbulent\nboundary layers has come into question in recent years. This is in\ncontrast to the traditionally accepted view that, upon appropriate\nnormalization, all three flows can be regarded as the same in the\nnear-wall region. In this paper, the authors aim to provide clarification\nof this issue through streamwise velocity measurements in these three\nflows with carefully matched Reynolds number and measurement resolution.\nResults show that mean statistics in the near-wall region collapse\nwell. However, the premultiplied energy spectra of streamwise velocity\nfluctuations show marked structural differences that cannot be explained\nby scaling arguments. It is concluded that, while similarities exist\nat these Reynolds numbers, one should exercise caution when drawing\ncomparisons between the three shear flows, even near the wall.},
author = {Monty, J. P. and Hutchins, N. and Ng, H. C.H. and Marusic, I. and Chong, M. S.},
doi = {10.1017/S0022112009007423},
file = {:C\:/Nutstore/Turbulence/paper/Monty 2009 a comparison of turbulent pipe channel and boundary layer flows.pdf:pdf},
issn = {00221120},
journal = {J. Fluid Mech.},
pages = {431--442},
title = {{A comparison of turbulent pipe, channel and boundary layer flows}},
volume = {632},
year = {2009}
}
