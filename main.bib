@article{Zhu2007,
abstract = {This paper presents a novel sensing methodology with an extended Kalman-based fusion algorithm for attitude estimation, using inexpensive micromachined gyroscopes, accelerometers and magnetometers. Unlike conventional methodology using quaternions and Euler angles, in the proposed fusion algorithm the state vector is defined to be a 6 × 1 vector containing sensing components of earth gravity and magnetic field in the body frame. By this way, the Kalman model can be represented by linear equations, which makes the iterative computations easy to be implemented at a faster rate using inexpensive microprocessors. The computation of the filter is further simplified by updating gravity and magnetic vectors respectively in smaller dimension. Experiments are performed to validate the effectiveness of the proposed approach. {\textcopyright} 2006 Elsevier Ltd. All rights reserved.},
author = {Zhu, Rong and Sun, Dong and Zhou, Zhaoying and Wang, Dingqu},
doi = {10.1016/j.measurement.2006.05.020},
file = {:C\:/Nutstore/Gyroscope/paper/Zhu a linear fusion algorithm for attitude determination using low cost MEMS-based sensors.pdf:pdf},
issn = {02632241},
journal = {Meas. J. Int. Meas. Confed.},
keywords = {Attitude determination,Electro-mechanical system (MEMS),Kalman-based fusion algorithm},
number = {3},
pages = {322--328},
title = {{A linear fusion algorithm for attitude determination using low cost MEMS-based sensors}},
volume = {40},
