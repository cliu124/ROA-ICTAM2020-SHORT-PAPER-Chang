file = {:C\:/Nutstore/Turbulence/paper/A Lagrangian fluctuation-dissipation relation for scalar turbulence. Part II. Wall-bounded flows.pdf:pdf},
issn = {14697645},
journal = {J. Fluid Mech.},
keywords = {turbulence theory,turbulent flows,turbulent mixing},
number = {1998},
pages = {153--189},
title = {{A Lagrangian fluctuation-dissipation relation for scalar turbulence. Part I. Flows with no bounding{\^{A}} walls}},
volume = {829},
year = {2017}
}
