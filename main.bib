@article{Vanon2018,
abstract = {{\textcopyright} 2018 Informa UK Limited, trading as Taylor  &  Francis Group The Navier–Stokes equations written in the vector potential can be recast as non-linear Schr{\"{o}}dinger equations at imaginary times, i.e. heat equations with a potential term, using the Cole–Hopf transform. On this basis, we study two kinds of Navier–Stokes flows by means of direct numerical simulations. In an experiment on vortex reconnection, it is found that the potential term takes large negative values in regions where intensive reconnection takes place, whereas the signature of the non-linear term is more broadly spread. For decaying turbulence starting from a random initial condition, such a correspondence is also observed in the early stage when the flow is dominated by vorticity layers. At later times, when the flow features several tubular vortices, this correspondence becomes weaker. Finally, a similar set of transformations is presented for the magneto–hydrodynamic equations, which reduces them to a set of heat equations with suitable potential terms, thereby obtaining new criteria for the regularity of their solutions.},
author = {Vanon, Riccardo and Ohkitani, Koji},
doi = {10.1080/14685248.2018.1431395},
file = {:C\:/Nutstore/Turbulence/paper/Applications of a Cole Hopf transform to the 3D Navier Stokes equations.pdf:pdf},
issn = {14685248},
journal = {J. Turbul.},
keywords = {Cole–Hopf transform,Duhamel principle,Feynman–Kac formula,Navier–Stokes equations},
number = {4},
pages = {322--333},
publisher = {Taylor & Francis},
title = {{Applications of a Cole–Hopf transform to the 3D Navier–Stokes equations}},
url = {https://doi.org/10.1080/14685248.2018.1431395},
volume = {19},
year = {2018}
}
