@article{Hutchins2009,
abstract = {Careful reassessment of new and pre-existing data shows that recorded scatter in the hot-wire-measured near-wall peak in viscous-scaled streamwise turbulence intensity is due in large part to the simultaneous competing effects of the Reynolds number and viscous-scaled wire length l+. An empirical expression is given to account for these effects. These competing factors can explain much of the disparity in existing literature, in particular explaining how previous studies have incorrectly concluded that the inner-scaled near-wall peak is independent of the Reynolds number. We also investigate the appearance of the so-called outer peak in the broadband streamwise intensity, found by some researchers to occur within the log region of high-Reynolds-number boundary layers. We show that the is consistent with the attenuation of small scales due to large l+. For turbulent boundary layers, in the absence of spatial resolution problems, there is no outer peak up to the Reynolds numbers investigated here (Re and for internal geometries maximum flow frequency1 scaling. In doing so, we are able to rationalize much of the disparity in pre-existing literature over the kx&8722;1 region of self-similarity. Not surprisingly, the attenuated spectra also indicate that Kolmogorov-scaled spectra are subject to substantial errors due to wire spatial resolution issues. These errors persist to wavelengths far beyond those which we might otherwise assume from simple isotropic assumptions of small-scale motions. The effects of hot-wire length-to-diameter ratio (l/d) are also briefly investigated. For the moderate wire Reynolds numbers investigated here, reducing l/d from 200 to 100 has a detrimental effect on measured turbulent fluctuations at a wide range of energetic scales, affecting both the broadband intensity and the energy spectra.},
author = {Hutchins, N. and Nickels, T. B. and Marusic, I. and Chong, M. S.},
doi = {10.1017/S0022112009007721},
file = {:C\:/Nutstore/Turbulence/paper/hutchins 2009div-class-title-hot-wire-spatial-resolution-issues-in-wall-bounded-turbulence-div.pdf:pdf},
issn = {00221120},
journal = {J. Fluid Mech.},
pages = {103--136},
title = {{Hot-wire spatial resolution issues in wall-bounded turbulence}},
volume = {635},
year = {2009}
}
