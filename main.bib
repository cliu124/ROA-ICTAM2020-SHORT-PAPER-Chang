keywords = {1,10,1137,15m1053347,34c28,34d05,90c22,93e03,ams subject classifications,and possibly chaotic,bounds,doi,dynamical systems with complicated,in the study of,introduction,stochastic expectations,sum-of-squares optimization,time averages},
number = {4},
pages = {1962--1988},
title = {{Bounds for deterministic and stochastic dynamical systems using sum-of-squares optimization}},
url = {http://arxiv.org/abs/1512.05599%0Ahttp://dx.doi.org/10.1137/15M1053347},
volume = {15},
