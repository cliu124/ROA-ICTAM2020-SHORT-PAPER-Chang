@article{Tomkins2005,
abstract = {The influence of large-scale outer-region motions on the properties\nand processes of the inner layer remains an open issue in wall turbulence\nresearch. In the present work, two-dimensional power spectra of the\nstreamwise component are measured in streamwise-spanwise planes throughout\nthe logarithmic region of smooth flat-plate turbulent boundary layers\nat Re-theta = 1015 and 7705. The spectra are based on PIV measurements\nwith a wide spanwise view (z/delta > 2.5), and the spanwise energy\ndistribution is emphasized. The spectra reveal that the mode associated\nwith the spacing of the low-speed streaks near the wall, lambda(z)(+)\napproximate to 100, contains surprisingly little energy relative\nto modes in the range lambda(z)(+) approximate to 200-400 at y(+)\n= 21. This result is consistent with measurements in a channel flow\n(Liu et al. 1996) at a similar height. Further from the wall, large-scale\nstructures that scale with outer variables organize with spacing\nlambda(z)/delta = 0.75-0.9, and these motions dominate the spanwise\ndistribution of streamwise energy throughout the logarithmic region.\nThe large spanwise modes are associated with the large streamwise\nmodes on average, as the median energetic spanwise mode increases\nroughly linearly with increasing streamwise mode up to approximately\nlambda(z.med)/delta approximate to 0.8, and then remains roughly\nconstant for larger streamwise modes. The aspect ratio lambda(x)/lambda(z,med)\ndecreases with increasing distance from the wall, suggesting that\nthe most streaky structures remain buried near the wall.},
author = {Tomkins, C. D. and Adrian, R. J.},
doi = {10.1017/S0022112005006397},
file = {:C\:/Nutstore/Turbulence/paper/energetic_spanwise_modes_in_the_logarithmic_layer_of_a_turbulent_boundary_layer.pdf:pdf},
issn = {00221120},
journal = {J. Fluid Mech.},
pages = {141--162},
title = {{Energetic spanwise modes in the logarithmic layer of a turbulent boundary layer}},
volume = {545},
year = {2005}
}
@article{Tol2017,
abstract = { A new framework is presented for estimation and control of instabilities in wall-bounded shear flows described by the linearised Navier–Stokes equations. The control design considers the use of localised actuators/sensors to account for convective instabilities in an ${\mathcal{H}}_{2}$ optimal control framework. External sources of disturbances are assumed to enter the control domain through the inflow. A new inflow disturbance model is proposed for external excitation of the perturbation modes that contribute to transition. This model allows efficient estimation of the flow perturbations within the localised control region of a conceptually unbounded domain. The state-space discretisation of the infinite-dimensional system is explicitly obtained, which allows application of linear control theoretic tools. A reduced-order model is subsequently derived using exact balanced truncation that captures the input/output behaviour and the dominant perturbation dynamics. This model is used to design an ${\mathcal{H}}_{2}$ optimal controller to suppress the instability growth. The two-dimensional non-periodic channel flow is considered as an application case. Disturbances are generated upstream of the control domain and the resulting flow perturbations are estimated/controlled using point wall shear measurements and localised unsteady blowing and suction at the wall. The controller is able to cancel the perturbations and is robust to both unmodelled disturbances and sensor inaccuracies. For single-frequency and multiple-frequency disturbances with low sensor noise a nearly full cancellation is achieved. For stochastic forced disturbances and high sensor noise an energy reduction in perturbation wall shear stress of 96 % is shown. },
author = {Tol, H. J. and Kotsonis, M. and {De Visser}, C. C. and Bamieh, B.},
doi = {10.1017/jfm.2017.355},
file = {:C\:/Nutstore/Turbulence/paper/Tol Bamieh localized estimation and control of linear instabilities in two-dimensional wall bounded shear flows.pdf:pdf},
issn = {14697645},
journal = {J. Fluid Mech.},
keywords = {control theory,flow control,instability control},
pages = {818--865},
title = {{Localised estimation and control of linear instabilities in two-dimensional wall-bounded shear flows}},
volume = {824},
year = {2017}
}
