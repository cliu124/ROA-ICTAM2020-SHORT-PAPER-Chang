keywords = {feynman-kac formula,integral inequalities,moment estimation,set avoidance,sum of squares},
number = {March},
title = {{Barrier Functionals for the Analysis of Complex Systems: An Optimization-Based Approach}},
url = {http://arxiv.org/abs/1603.08716},
year = {2016}
}
