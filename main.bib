keywords = {Bracket formulation,Discrete variational derivative method,Mimetic finite difference method,Three-dimensional vorticity equation},
pages = {174--225},
publisher = {Elsevier B.V.},
title = {{Bracket formulations and energy- and helicity-preserving numerical methods for the three-dimensional vorticity equation}},
