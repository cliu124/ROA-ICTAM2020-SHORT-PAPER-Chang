pages = {1--11},
title = {{Network Structure of Two-Dimensional Decaying Isotropic Turbulence}},
url = {http://arxiv.org/abs/1705.07526%0Ahttp://dx.doi.org/10.1017/jfm.2016.235},
year = {2017}
}
