journal = {J. Fluid Mech.},
keywords = {turbulence simulation,turbulent boundary layers,turbulent flows},
pages = {171--187},
title = {{Spectra of energy transport in turbulent channel flows for moderate Reynolds numbers}},
volume = {805},
year = {2016}
}
