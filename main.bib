@article{Moehlis2005,
abstract = {We consider the dynamics of a low-dimensional model for turbulent\nshear °ows. The model is based on Fourier modes and describes sinusoidal\nshear °ow, in which °uid between two free-slip walls experiences\na sinusoidal body force. The model contains nine modes, most of which\nhave a direct hydrodynamical interpretation. We analyze the stationary\nstates and periodic orbits for the model for two di{\textregistered}erent domain\nsizes. Several kinds of bifurcations are identi¯ed, including saddle-node\nbifurcations, a period doubling cascade, and Hopf bifurcations of\nthe periodic orbits. For both domain sizes, long-lived transient\nchaos appears to be associated with the presence of a large number\nof unstable periodic orbits. For the smaller minimal °ow unit domain,\nit is found that a periodic solution is stable over a range of Reynolds\nnumbers, and its bifurcations lead to the existence of a chaotic\nattractor. The model illustrates many phenomena observed and speculated\nto exist in the transition to turbulence in linearly stable shear\n°ows.},
author = {Moehlis, Jeff and Faisst, Holger and Eckhardt, Bruno},
doi = {10.1137/040606144},
file = {:C\:/Nutstore/Turbulence/paper/Moehlis 2005 periodic orbits and chaotic sets in a low-dimensional model for shear flows.pdf:pdf},
journal = {SIAM J. Appl. Dyn. Syst.},
keywords = {040606144,1,10,1137,37d45,37g35,70k50,76f20,ams subject classifications,bifurcations,chaotic sets,doi,in discussing the transition,introduction,one may broadly distinguish,periodic orbits,shear flow turbulence,to turbulence},
number = {2},
pages = {352--376},
title = {{Periodic Orbits and Chaotic Sets in a Low-Dimensional Model for Shear Flows}},
volume = {4},
year = {2005}
}
@article{Moehlis2004,
abstract = {We analyse a low-dimensional model for turbulent shear flows. The model is based on Fourier modes and describes sinusoidal shear flow, in which the fluid between two free-slip walls experiences a sinusoidal body force. The model contains a total of nine modes, including modes describing the basic mean velocity profile and its modification, downstream vortices, streaks, and instabilities of streaks, with other modes being a consequence of the non-linear interactions. The transition to turbulence for the model is subcritical and intermittent, and the distributions of turbulent lifetimes are exponential, in agreement with observations in many shear flows.},
author = {Moehlis, Jeff and Faisst, Holger and Eckhardt, Bruno},
file = {:C\:/Nutstore/Turbulence/paper/a low-dimensional model for turbulent shear flows.pdf:pdf},
journal = {New J. Phys.},
